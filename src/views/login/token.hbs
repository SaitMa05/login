<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Token</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="../alert/toastr.min.css">    
    <link rel="stylesheet" href="/style.css">
    <link rel="stylesheet" href="/form.css">
</head>
<body>
    <main class="main">
        <div class="register">
            <form action="" class="form" method="POST">
                {{#if error}}
                    <div class="alert alert-danger mt-2 text-center" role="alert">
                        {{error}}
                    </div>
                {{/if}}
                <h2>Token</h2>
                <div class="form-group">

                    <div>
                        <label for="token">Token:</label>
                        <input type="number" name="token" id="token" placeholder="Ingrese su token" value="" required>
                    </div>
                </div>
                <button type="submit" class="btn font-bold btn-warning text-white">Verificar</button>



            </form>
        </div>
    </main>

    <script src="/register.js"></script>

    

    <!-- <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script> -->
</body>
</html>